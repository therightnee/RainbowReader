;a:1:{s:0:"";a:1:{s:6:"domain";s:45:"http://www.popsci.com/category/tags/addiction";}}s:8:"xml_base";s:0:"";s:17:"xml_base_explicit";b:0;s:8:"xml_lang";s:0:"";}i:8;a:5:{s:4:"data";s:14:"hypersexuality";s:7:"attribs";a:1:{s:0:"";a:1:{s:6:"domain";s:50:"http://www.popsci.com/category/tags/hypersexuality";}}s:8:"xml_base";s:0:"";s:17:"xml_base_explicit";b:0;s:8:"xml_lang";s:0:"";}i:9;a:5:{s:4:"data";s:7:"Science";s:7:"attribs";a:1:{s:0:"";a:1:{s:6:"domain";s:43:"http://www.popsci.com/category/tags/science";}}s:8:"xml_base";s:0:"";s:17:"xml_base_explicit";b:0;s:8:"xml_lang";s:0:"";}}s:7:"pubDate";a:1:{i:0;a:5:{s:4:"data";s:29:"Wed, 17 Jul 2013 20:00:00 GMT";s:7:"attribs";a:0:{}s:8:"xml_base";s:0:"";s:17:"xml_base_explicit";b:0;s:8:"xml_lang";s:0:"";}}s:4:"guid";a:1:{i:0;a:5:{s:4:"data";s:30:"75890 at http://www.popsci.com";s:7:"attribs";a:1:{s:0:"";a:1:{s:11:"isPermaLink";s:5:"false";}}s:8:"xml_base";s:0:"";s:17:"xml_base_explicit";b:0;s:8:"xml_lang";s:0:"";}}}s:32:"http://purl.org/dc/elements/1.1/";a:1:{s:7:"creator";a:1:{i:0;a:5:{s:4:"data";s:14:"Shaunacy Ferro";s:7:"attribs";a:0:{}s:8:"xml_base";s:0:"";s:17:"xml_base_explicit";b:0;s:8:"xml_lang";s:0:"";}}}}}i:17;a:6:{s:4:"data";s:0:"";s:7:"attribs";a:0:{}s:8:"xml_base";s:0:"";s:17:"xml_base_explicit";b:0;s:8:"xml_lang";s:0:"";s:5:"child";a:2:{s:0:"";a:6:{s:5:"title";a:1:{i:0;a:5:{s:4:"data";s:55:"A Brief History Of Architectural Blunders [Infographic]";s:7:"attribs";a:0:{}s:8:"xml_base";s:0:"";s:17:"xml_base_explicit";b:0;s:8:"xml_lang";s:0:"";}}s:4:"link";a:1:{i:0;a:5:{s:4:"data";s:165:"http://feeds.popsci.com/c/34567/f/632419/s/2ecd76db/l/0L0Spopsci0N0Ctechnology0Carticle0C20A130E0A70Cbrief0Ehistory0Earchitectural0Eblunders0Einfographic/story01.htm";s:7:"attribs";a:0:{}s:8:"xml_base";s:0:"";s:17:"xml_base_explicit";b:0;s:8:"xml_lang";s:0:"";}}s:11:"description";a:1:{i:0;a:5:{s:4:"data";s:3906:"<div class="center-image"><img src="http://www.popsci.com/files/imagecache/article_image_large/articles/architecture-fails.jpg" alt="" title="" class="imagecache imagecache-article_image_large" /><div><strong>Failure By Design</strong> <em><p>NewSchool of Architecture and Design</p> </em></div></div><div>Maybe if you're putting a building in the Windy City it should be wind-proof? <!--paging_filter--><p><!--break-->This infographic, published by <a href="http://www.newschoolarch.edu/news-events/FailurebyDesign.htm">the NewSchool of Architecture and Design</a>, ticks off some of history's most famous architectural failures, reaching back to Ancient Rome's Fidenae Ampitheatre (collapsed in 27 A.D.) all the way to the Lotus Riverside apartment complex in Shanghai (collapsed in 2009). At least the infographic includes some good news, pulling lessons learned out of the events. (Although! Isn't the lesson from the Tower of Pisa that architectural defects can be the best?)</p> <p>Some others we might've included: Frank Gehry's <a href="http://web.mit.edu/facilities/construction/completed/stata.html">Ray and Maria Stata Center at MIT</a>, a warped cartoon of a building that's been plagued by cracks and other problems; and Norman Foster's <a href="http://en.wikipedia.org/wiki/The_Harmon">the Harmon</a>, the Las Vegas highrise that, because of structural flaws, was never used and is supposed to be demolished by explosion. A <i>different</i> Vegas hotel actually made the infographic, though, so read on. </p> <p></p> <p>[<a href="http://www.newschoolarch.edu/news-events/FailurebyDesign.htm">The NewSchool of Architecture and Design</a> via <a href="http://visual.ly/failure-design">visual.ly</a>]</p> </div><img width='1' height='1' src='http://feeds.popsci.com/c/34567/f/632419/s/2ecd76db/mf.gif' border='0'/><div class='mf-viral'><table border='0'><tr><td valign='middle'><a href="http://share.feedsportal.com/share/twitter/?u=http%3A%2F%2Fwww.popsci.com%2Ftechnology%2Farticle%2F2013-07%2Fbrief-history-architectural-blunders-infographic&t=A+Brief+History+Of+Architectural+Blunders+%5BInfographic%5D" targ